{
  "name": "cfl-converter",
  "version": "2.0.0",
  "description": "Convert IMVU's CFL format to CHKN (zip)",
  "main": "build/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:js && pkg build/cfl-converter.js --out-path ./bin",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "tsc",
    "test": "exit 0",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "build/",
    "src/",
    "test/",
    "typings/"
  ],
  "bin": {
    "cfl-converter": "./build/cfl-converter.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhkatz/cfl-converter.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "keywords": [
    "imvu",
    "cfl",
    "chkn",
    "converter"
  ],
  "author": {
    "name": "David Katz",
    "url": "https://github.com/dhkatz"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhkatz/cfl-converter/issues"
  },
  "homepage": "https://github.com/dhkatz/cfl-converter#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/jszip": "^3.4.1",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.4",
    "pkg": "^5.3.2",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "csharp-binary-stream": "^1.0.3",
    "get-stream": "^6.0.1",
    "jszip": "^3.7.1",
    "lzma-purejs": "github:elderapo/lzma-purejs"
  },
  "optionalDependencies": {
    "@types/download": "^8.0.1",
    "commander": "^8.2.0",
    "download": "^8.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {}
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.json"
      }
    },
    "testEnvironment": "node",
    "collectCoverage": true,
    "testRegex": "/test/.*\\.test\\.[jt]s$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!<rootDir>/node_modules/"
    ]
  }
}
